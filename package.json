{
  "name": "serp",
  "version": "0.0.1",
  "description": "brak",
  "license": "MIT",
  "repository": "https://github.com/pantalejmon/apsi",
  "author": "DM, AJ, JJ, MK, OM",
  "keywords": [
    "APSI"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build-clean": "npm run clean && tsc",
    "postinstall": "npm run build",
    "go": "tsc && node dist/index.js",
    "dev": "nodemon",
    "watch": "tsc --watch",
    "test": "mocha --opts tests/mocha.opts",
    "test-watch": "mocha --opts tests/mocha.opts --watch",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "mocha": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "engines": {
    "node": ">=6.10"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.16",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/sendmail": "^1.4.2",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "help": "^3.0.2",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.14.0",
    "sendmail": "^1.6.1",
    "typeorm": "^0.2.20",
    "typeorm-store": "^1.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run go",
    "ext": "ts"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
